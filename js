<script type="text/javascript">
	var swiper = new Swiper(".swiper", {
    slidesPerView: 9,
    spaceBetween: 3,
    slidesPerGroup: 9,
    loop: true,
    loopFillGroupWithBlank: true,
    navigation: {
      nextEl: ".swiper-button-next",
      prevEl: ".swiper-button-prev",
    },
  });
</script>